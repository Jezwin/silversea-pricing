<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:mixinTypes="[mix:lockable]"
    jcr:primaryType="cq:Template"
    jcr:title="SSC - Blog posts"
    jcr:description="Allows to create a new blog posts"
    allowedPaths="[/content/silversea-com/.*]"
    ranking="{Long}0">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="silversea/silversea-com/components/pages/blogposts">
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/libs/wcm/foundation/components/responsivegrid">
            <display
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/apps/silversea/silversea-com/components/displayproperties/blogproperties"
                text="Display properties" />
        </par>
    </jcr:content>
</jcr:root>