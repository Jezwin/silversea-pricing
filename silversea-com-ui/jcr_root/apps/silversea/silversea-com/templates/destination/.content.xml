<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Silversea.com destination template"
    jcr:mixinTypes="[mix:lockable]"
    jcr:primaryType="cq:Template"
    jcr:title="SSC - Destination"
    allowedPaths="[/content/silversea-com/.*]"
    ranking="{Long}0">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="silversea/silversea-com/components/pages/destination">
        <par jcr:primaryType="nt:unstructured" sling:resourceType="/libs/wcm/foundation/components/responsivegrid"
            components="[group:displayproperties]">
            <display
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/apps/silversea/silversea-com/components/displayproperties/destinationproperties"
                text="Display properties" />
        </par>
    </jcr:content>
</jcr:root>
