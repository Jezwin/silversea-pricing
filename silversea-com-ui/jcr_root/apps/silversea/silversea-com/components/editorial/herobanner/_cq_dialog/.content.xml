<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="SSC - Hero banner"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav" />
        <items jcr:primaryType="nt:unstructured">
            <slider
                jcr:primaryType="nt:unstructured"
                jcr:title="Hero Banner"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Title"
                                name="./title"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext" />
                            <description
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Description"
                                name="./jcr:description"
                                sling:resourceType="granite/ui/components/foundation/form/textfield" />
                            <background
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Background"
                                name="./assetReference"
                                rootPath="/content/dam/siversea-com"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser" />
                            <video
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Video"
                                name="./videoReference"
                                rootPath="/content/dam/siversea-com"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser" />
                            <disabledTextBackground
                                jcr:primaryType="nt:unstructured"
                                text="Disable text background"
                                name="./disabledTextBackground"
                                value="true"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox" />
                        </items>
                    </column>
                </items>
            </slider>
            <button1
                jcr:primaryType="nt:unstructured"
                jcr:title="Button 1"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <desktopTitle
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Desktop Title"
                                name="./button1/titleDesktop"
                                sling:resourceType="granite/ui/components/foundation/form/textfield" />
                            <tabletTitle
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Tablet Title"
                                name="./button1/titleTablet"
                                sling:resourceType="granite/ui/components/foundation/form/textfield" />
                            <urlLink
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="URL Link"
                                name="./button1/reference"
                                rootPath="/content/silversea-com"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser" />
                            <color
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Color"
                                name="./button1/color">
                                <items jcr:primaryType="nt:unstructured">
                                    <gray
                                        jcr:primaryType="nt:unstructured"
                                        text="gray"
                                        value="gray" />
                                    <red
                                        jcr:primaryType="nt:unstructured"
                                        text="red"
                                        value="red" />
                                </items>
                            </color>
                            <size
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Size"
                                name="./button1/size">
                                <items jcr:primaryType="nt:unstructured">
                                    <normal
                                        jcr:primaryType="nt:unstructured"
                                        text="normal"
                                        value="" />
                                    <small
                                        jcr:primaryType="nt:unstructured"
                                        text="small"
                                        value="small" />
                                </items>
                            </size>
                            <analyticType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Analytic type"
                                name="./button1/analyticType">
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text="Simple button"
                                        value="" />
                                    <raq
                                        jcr:primaryType="nt:unstructured"
                                        text="Quote request"
                                        value="clic-RAQ" />
                                    <bro
                                        jcr:primaryType="nt:unstructured"
                                        text="Brochure request"
                                        value="clic-BRO" />
                                    <rac
                                        jcr:primaryType="nt:unstructured"
                                        text="Call request"
                                        value="clic-RAC" />
                                    <sfo
                                        jcr:primaryType="nt:unstructured"
                                        text="Signup for offer"
                                        value="clic-SFO" />
                                </items>
                            </analyticType>
                        </items>
                    </column>
                </items>
            </button1>
            <button2
                jcr:primaryType="nt:unstructured"
                jcr:title="Button 2"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <desktopTitle
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Desktop Title"
                                name="./button2/titleDesktop"
                                sling:resourceType="granite/ui/components/foundation/form/textfield" />
                            <tabletTitle
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Tablet Title"
                                name="./button2/titleTablet"
                                sling:resourceType="granite/ui/components/foundation/form/textfield" />
                            <urlLink
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="URL Link"
                                name="./button2/reference"
                                rootPath="/content/silversea-com"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser" />
                            <color
                                jcr:primaryType="nt:unstructured"
                                fieldLabel="Color"
                                name="./button2/color"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select">
                                <items jcr:primaryType="nt:unstructured">
                                    <gray
                                        jcr:primaryType="nt:unstructured"
                                        text="gray"
                                        value="gray" />
                                    <red
                                        jcr:primaryType="nt:unstructured"
                                        text="red"
                                        value="red" />
                                </items>
                            </color>
                            <size
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Size"
                                name="./button2/size">
                                <items jcr:primaryType="nt:unstructured">
                                    <normal
                                        jcr:primaryType="nt:unstructured"
                                        text="normal"
                                        value="" />
                                    <small
                                        jcr:primaryType="nt:unstructured"
                                        text="small"
                                        value="small" />
                                </items>
                            </size>
                            <analyticType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Analytic type"
                                name="./button2/analyticType">
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text="Simple button"
                                        value="" />
                                    <raq
                                        jcr:primaryType="nt:unstructured"
                                        text="Quote request"
                                        value="clic-RAQ" />
                                    <bro
                                        jcr:primaryType="nt:unstructured"
                                        text="Brochure request"
                                        value="clic-BRO" />
                                    <rac
                                        jcr:primaryType="nt:unstructured"
                                        text="Call request"
                                        value="clic-RAC" />
                                    <sfo
                                        jcr:primaryType="nt:unstructured"
                                        text="Signup for offer"
                                        value="clic-SFO" />
                                </items>
                            </analyticType>
                        </items>
                    </column>
                </items>
            </button2>
        </items>
    </content>
</jcr:root>