<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="SSC - Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav" />
        <items jcr:primaryType="nt:unstructured">
            
            <references
                jcr:primaryType="nt:unstructured"
                jcr:title="References"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <mainCol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="References">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Reference"
                                    name="./reference"
                                    rootPath="/content/silversea-com" />
                            </mainCol>
                            <subcol1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="subCol1"
                                fieldLabel="Column 1"
                                name="./subCol1"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <subcol2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="subCol2"
                                fieldLabel="Column 2"
                                name="./subCol2"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <subcol3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="subCol3"
                                fieldLabel="Column 3"
                                name="./subCol3"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <mysilverseareference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="mySilverseaReference"
                                fieldLabel="My Silversea"
                                name="./mySilverseaReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                        </items>
                    </column>
                </items>
            </references>
    
            <socialandaction
                jcr:primaryType="nt:unstructured"
                jcr:title="Social actions"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <quoteReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="quoteReference"
                                fieldLabel="Quote request"
                                name="./quoteReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <exclusiveOfferReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="exclusiveOfferReference"
                                fieldLabel="Exclusive offers"
                                name="./exclusiveOfferReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <awardReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="awardReference"
                                fieldLabel="Awards page"
                                name="./awardReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <facebookReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="facebookReference"
                                fieldLabel="Facebook"
                                name="./facebookReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <youtubeReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="youtubeReference"
                                fieldLabel="Youtube"
                                name="./youtubeReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <twitterReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="twitterReference"
                                fieldLabel="Twitter"
                                name="./twitterReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <instagramReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="instagramReference"
                                fieldLabel="Instagram"
                                name="./instagramReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <pinterestReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="pinterestReference"
                                fieldLabel="Pinterest"
                                name="./pinterestReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                            <blogReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="blogReference"
                                fieldLabel="Blog"
                                name="./blogReference"
                                renderReadOnly="{Boolean}true"
                                rootPath="/content/silversea-com" />
                        </items>
                    </column>
                </items>
            </socialandaction>
    
            <Legalline
                jcr:primaryType="nt:unstructured"
                jcr:title="Legal line"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <mainCol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="References">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Reference"
                                    name="./referencelegal"
                                    rootPath="/content/silversea-com" />
                            </mainCol>
                        </items>
                    </column>
                </items>
            </Legalline>
        </items>
    </content>
</jcr:root>