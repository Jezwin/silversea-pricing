<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="SSC - Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav" />
        <items jcr:primaryType="nt:unstructured">
            <references
                jcr:primaryType="nt:unstructured"
                jcr:title="References"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <mainCol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="References">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Reference"
                                    name="./reference"
                                    rootPath="/content/silversea-com" />
                            </mainCol>
                            <subcol1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Column 1"
                                name="./subCol1"
                                rootPath="/content/silversea-com" />
                            <subcol2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Column 2"
                                name="./subCol2"
                                rootPath="/content/silversea-com" />
                            <subcol3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Column 3"
                                name="./subCol3"
                                rootPath="/content/silversea-com" />
                            <mysilverseareference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="My Silversea"
                                name="./mySilverseaReference"
                                rootPath="/content/silversea-com" />
                            <hideGreySectionFooter
                               jcr:primaryType="nt:unstructured"
                               text="Hide More About Silversea section"
                               name="./hideGreySectionFooter"
                               value="true"
                               sling:resourceType="granite/ui/components/foundation/form/checkbox" />
                        </items>
                    </column>
                </items>
            </references>
            <socialandaction
                jcr:primaryType="nt:unstructured"
                jcr:title="Social actions"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <quoteReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Quote request"
                                name="./quoteReference"
                                rootPath="/content/silversea-com" />
                            <exclusiveOfferReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Exclusive offers"
                                name="./exclusiveOfferReference"
                                rootPath="/content/silversea-com" />
                            <awardReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Awards page"
                                name="./awardReference"
                                rootPath="/content/silversea-com" />
                            <facebookReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Facebook"
                                name="./facebookReference" />
                            <youtubeReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Youtube"
                                name="./youtubeReference" />
                            <twitterReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Twitter"
                                name="./twitterReference" />
                            <instagramReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Instagram"
                                name="./instagramReference" />
                            <pinterestReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Pinterest"
                                name="./pinterestReference" />
                            <blogReference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Blog"
                                name="./blogReference"
                                rootPath="/content/silversea-com" />
                        </items>
                    </column>
                </items>
            </socialandaction>
            <Legalline
                jcr:primaryType="nt:unstructured"
                jcr:title="Legal line"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false" />
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <mainCol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="References">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Reference"
                                    name="./referencelegal"
                                    rootPath="/content/silversea-com" />
                            </mainCol>
                        </items>
                    </column>
                </items>
            </Legalline>
            <footerbversion
                jcr:primaryType="nt:unstructured"
                jcr:title="Footer B version"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <brochureimage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Brochure Image"
                                name="./brochureimage"
                                rootPath="/content/dam"/>
                            <Col1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldDescription="Ignored for Mobile view"
                                fieldLabel="Column 1 (hidden on mobile)">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Col 1"
                                    name="./col1"
                                    rootPath="/content/silversea-com"/>
                            </Col1>
                            <Col2title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Column 2 Title (mobile only)"
                                fieldLabel="Column 2 Title (mobile only)"
                                name="./Col2title"/>
                            <Col2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Column 2">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Col 2"
                                    name="./col2"
                                    rootPath="/content/silversea-com"/>
                            </Col2>
                            <Col3title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Column 3 Title (mobile only)"
                                fieldLabel="Column 3 Title (mobile only)"
                                name="./Col3title"/>
                            <Col3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Column 3">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Col 3"
                                    name="./col3"
                                    rootPath="/content/silversea-com"/>
                            </Col3>
                            <Col4title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Column 4 Title (mobile only)"
                                fieldLabel="Column 4 Title (mobile only)"
                                name="./Col4title"/>
                            <Col4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Column 4">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Col 4"
                                    name="./col4"
                                    rootPath="/content/silversea-com"/>
                            </Col4>
                        </items>
                    </column>
                </items>
            </footerbversion>
            <requestBrochure
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Request brochure"
                    sling:resourceType="granite/ui/components/foundation/section">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                                <assetImageBrochure
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Brochure Image"
                                    name="./assetImageBrochure"
                                    rootPath="/content/dam"/>
                               <linkCtaBrochure
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                    fieldLabel="Brochure Url Cta"
                                    name="./linkCtaBrochure"
                                    rootPath="/content/silversea-com"/>
                        </items>
                    </column>
                </items>
            </requestBrochure>
        </items>
    </content>
</jcr:root>