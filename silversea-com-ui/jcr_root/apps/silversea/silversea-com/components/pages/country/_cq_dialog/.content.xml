<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <port
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Port"
                            cq:showOnCreate="{Boolean}false"
                            sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <id
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="id"
                                            fieldLabel="Id"
                                            name="./id"
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <phoneNumber
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="phoneNumber"
                                            fieldLabel="Phone Number"
                                            allowBulkEdit="{Boolean}true"
                                            name="./phoneNumber" />
                                    <name
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="name"
                                            fieldLabel="Name"
                                            name="./name" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <url
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="url"
                                            fieldLabel="URL"
                                            name="./url" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <iso2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="iso2"
                                            fieldLabel="Iso 2"
                                            name="./iso2" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <iso3
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="iso3"
                                            fieldLabel="Iso 3"
                                            name="./iso3" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <prefix
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="prefix"
                                            fieldLabel="Prefix"
                                            name="./prefix" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <market
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="market"
                                            fieldLabel="Market"
                                            name="./market" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <region
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="region"
                                            fieldLabel="Region"
                                            name="./region" 
                                            allowBulkEdit="{Boolean}true"
                                            renderReadOnly="{Boolean}true"
                                            disabled="{Boolean}true"/>
                                    <openHour
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="openHour"
                                            fieldLabel="Open Hour"
                                            allowBulkEdit="{Boolean}true"
                                            name="./openHour"/>
                                    <closeHour
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="closeHour"
                                            fieldLabel="Close Hour"
                                            allowBulkEdit="{Boolean}true"
                                            name="./closeHour"/>
                                    <openHourSaturday
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="openHourSaturday"
                                            fieldLabel="Open Hour Saturday"
                                            name="./openHourSaturday"/>
                                    <closeHourSaturday
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="closeHourSaturday"
                                            fieldLabel="Close Hour Saturday"
                                            allowBulkEdit="{Boolean}true"
                                            name="./closeHourSaturday"/>
                                    <currency
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            cq-msm-lockable="currency"
                                            fieldLabel="Currency"
                                            allowBulkEdit="{Boolean}true"
                                            name="./currency"/>
                                </items>
                            </column>
                        </items>
                    </port>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
