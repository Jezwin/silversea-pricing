<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="SSC - Destination">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <destination
                        jcr:primaryType="nt:unstructured"
                        jcr:title="destination"
                        cq:showOnCreate="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <mapLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="mapLabel"
                                        fieldLabel="Map Label"
                                        name="./mapLabel"/>
                                    <excerpt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="Excerpt"
                                        fieldLabel="Excerpt"
                                        name="./excerpt"/>
                                    <destinationId
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/foundation/form/numberfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="destinationId"
                                        fieldLabel="Destination Id"
                                        name="./destinationId"
                                        disabled="{Boolean}true"
                                        required="{Boolean}true"/>
                                    <ragTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="ragTitle"
                                        fieldLabel="RAQ Title"
                                        name="./raqTitle"/>
                                    <longdescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        useFixedInlineToolbar="{Boolean}true"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="longDescription"
                                        fieldLabel="Long Description"
                                        name="./longDescription"/>
                                    <destinationFareAdditionsClassic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="destinationFareAdditionsClassic"
                                        fieldLabel="Destination fare additions classic"
                                        name="./destinationFareAdditionsClassic" />
                                    <destinationFareAdditionsExpedition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="destinationFareAdditionsExpedition"
                                        fieldLabel="Destination fare additions expedition"
                                        name="./destinationFareAdditionsExpedition" />
                                    <footnote
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="footnote"
                                        fieldLabel="Foot note"
                                        name="./footnote"/>
                                    <assetselectionreference
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="assetSelectionReference"
                                        fieldLabel="Extra Media Gallery"
                                        rootPath="/content/dam/silversea-com"
                                        name="./assetSelectionReference"/>
                                    <customHtml
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="customHtml"
                                        fieldLabel="Discover more"
                                        name="./customHtml"/>
                                </items>
                            </column>
                        </items>
                    </destination>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>