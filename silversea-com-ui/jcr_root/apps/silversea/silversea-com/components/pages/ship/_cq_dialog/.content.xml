<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <ship
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Ship"
                            cq:showOnCreate="{Boolean}false"
                            sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <longdescription
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                            useFixedInlineToolbar="{Boolean}true"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="longDescription"
                                            fieldLabel="Long Description"
                                            name="./longDescription"
                                            renderReadOnly="{Boolean}true"/>
                                    <excerpt
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                            fieldLabel="Excerpt"
                                            name="./excerpt"/>
                                    <shipcode
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="shipCode"
                                            fieldLabel="Ship code"
                                            name="./shipCode"
                                            renderReadOnly="{Boolean}true"/>
                                    <shipid
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="shipId"
                                            fieldLabel="Ship Id"
                                            name="./shipId"
                                            renderReadOnly="{Boolean}true"/>
                                    <ragTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="ragTitle"
                                        fieldLabel="RAQ Title"
                                        name="./raqTitle"/>
                                    <heroBanner
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="bannerReference"
                                            fieldLabel="Hero Banner"
                                            name="./bannerReference"
                                            rootPath="/content/dam/silversea-com"
                                            renderReadOnly="{Boolean}true"/>
                                    <assetSelectionReference
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="assetSelectionReference"
                                            fieldLabel="Extra Media Gallery"
                                            name="./assetSelectionReference"
                                            rootPath="/content/dam/silversea-com"
                                            renderReadOnly="{Boolean}true"/>
                                    <deckplan
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="deckPlan"
                                            fieldLabel="Global Deck Plan"
                                            name="./deckPlan"
                                            rootPath="/content/dam/silversea-com"
                                            renderReadOnly="{Boolean}true"/>
                                   <photoVideoSuiteSelectionReference
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="photoVideoSuiteSelectionReference"
                                            fieldLabel="Photo and Video Suite Media Gallery"
                                            name="./photoVideoSuiteSelectionReference"
                                            rootPath="/content/dam/silversea-com"
                                            renderReadOnly="{Boolean}true"/>
                                    <shortVoyageDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="shortVoyageDescription"
                                        rows="10"
                                        fieldLabel="Short Voyage Description"
                                        name="./shortVoyageDescription"
                                        renderReadOnly="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </ship>
                    <facts
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Facts"
                            cq:showOnCreate="{Boolean}false"
                            sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <guestsCapacity
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="guestsCapacity"
                                            fieldLabel="Guests Capacity"
                                            name="./guestsCapacity"
                                            renderReadOnly="{Boolean}true"/>
                                    <crewCapacity
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="crewCapacity"
                                            fieldLabel="Crew Capacity"
                                            name="./crewCapacity"
                                            renderReadOnly="{Boolean}true"/>
                                    <officersCapacity
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="officersCapacity"
                                            fieldLabel="Officers Capacity"
                                            name="./officersCapacity"
                                            renderReadOnly="{Boolean}true"/>
                                    <builtDate
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/datepicker"
                                            allowBulkEdit="{Boolean}true"
                                            displayedFormat="MMMM DD, YYYY hh:mm a"
                                            displayTimezoneMessage="{Boolean}true"
                                            type="datetime"
                                            cq-msm-lockable="builtDate"
                                            fieldLabel="Built Date"
                                            name="./builtDate"
                                            renderReadOnly="{Boolean}true"/>
                                    <refurbDate
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/datepicker"
                                            allowBulkEdit="{Boolean}true"
                                            displayedFormat="MMMM DD, YYYY hh:mm a"
                                            displayTimezoneMessage="{Boolean}true"
                                            type="datetime"
                                            cq-msm-lockable="refurbDate"
                                            fieldLabel="Refurb date"
                                            name="./refurbDate"
                                            renderReadOnly="{Boolean}true"/>
                                    <tonnage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="tonnage"
                                            fieldLabel="Tonnage"
                                            name="./tonnage"
                                            renderReadOnly="{Boolean}true"/>
                                    <lengthFt
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="lengthFt"
                                            fieldLabel="Length ft"
                                            name="./lengthFt"
                                            renderReadOnly="{Boolean}true"/>
                                    <lengthM
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="lengthM"
                                            fieldLabel="Length m"
                                            name="./lengthM"
                                            renderReadOnly="{Boolean}true"/>
                                    <widthFt
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="widthFt"
                                            fieldLabel="WidthFt"
                                            name="./widthFt"
                                            renderReadOnly="{Boolean}true"/>
                                    <widthM
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="widthM"
                                            fieldLabel="WidthM"
                                            name="./widthM"
                                            renderReadOnly="{Boolean}true"/>
                                    <speed
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="speed"
                                            fieldLabel="Speed"
                                            name="./speed"
                                            renderReadOnly="{Boolean}true"/>
                                    <registry
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="registry"
                                            fieldLabel="Registry"
                                            name="./registry"
                                            renderReadOnly="{Boolean}true"/>
                                    <connectingSuites
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="connectingSuites"
                                            fieldLabel="Connectings suties"
                                            name="./connectingSuites"
                                            renderReadOnly="{Boolean}true"/>
                                    <handicapSuites
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="handicapSuites"
                                            fieldLabel="Handicap suties"
                                            name="./handicapSuites"
                                            renderReadOnly="{Boolean}true"/>
                                    <thirdGuestCapacity
                                            jcr:primaryType="nt:unstructured"
                                            text="3rd Guest Capacity"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="thirdGuestCapacity"
                                            name="./thirdGuestCapacity"
                                            value="true"
                                            sling:resourceType="granite/ui/components/foundation/form/checkbox" />
                                    <deckInfoNode
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/multifield"
                                            class="full-width"
                                            fieldLabel="Deck Info">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            acs-commons-nested="NODE_STORE"
                                            name="./deckInfoNode">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                method="absolute"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <deckLevel
                                                            jcr:primaryType="nt:unstructured"
                                                            fieldLabel="Deck level"
                                                            name="./deckLevel"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield" />
                                                        <deckImageTop
                                                                jcr:primaryType="nt:unstructured"
                                                                fieldLabel="Deck image"
                                                                name="./deckImageTop"
                                                                rootPath="/content/dam/silversea-com"
                                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser" />
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </deckInfoNode>
                                </items>
                            </column>
                        </items>
                    </facts>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>