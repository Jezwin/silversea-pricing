<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <diningvariation
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Dining variation"
                            cq:showOnCreate="{Boolean}false"
                            sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <diningreference
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="diningReference"
                                            fieldLabel="Dining reference"
                                            name="./diningReference"
                                            required="{Boolean}true"
                                            rootPath="/content/silversea-com"
                                            renderReadOnly="{Boolean}true"/>
                                    <plan
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="plan"
                                            fieldLabel="Plan"
                                            name="./plan"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/dam/silversea-com"/>
                                    <virtualtour
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="virtualTour"
                                            fieldLabel="Virtual Tour"
                                            name="./virtualTour"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/dam/silversea-com"/>
                                    <locationImage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="locationImage"
                                            fieldLabel="Location Image"
                                            name="./locationImage"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/dam/silversea-com"/>
                                    <shortVoyageDescription
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="shortVoyageDescription"
                                            rows="10"
                                            fieldLabel="Short Voyage Description"
                                            name="./shortVoyageDescription"
                                            renderReadOnly="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </diningvariation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
