<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Title and Tags"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagename
                                                cq:hideOnEdit="{Boolean}true"
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Name"
                                                name="pageName"
                                                validation="foundation.jcr.name"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                cq-msm-lockable="jcr:title"
                                                fieldLabel="Title"
                                                name="./jcr:title"
                                                renderReadOnly="{Boolean}true"
                                                required="{Boolean}true"/>
                                            <msmConfigEndpoint
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                class="msm-config-endpoint"
                                                value="${empty param.item ? requestPathInfo.suffix : param.item }"/>
                                            <tags
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/common/tagspicker"
                                                allowBulkEdit="{Boolean}true"
                                                allowCreate="{Boolean}true"
                                                cq-msm-lockable="cq:tags"
                                                fieldLabel="Tags"
                                                name="./cq:tags"
                                                renderReadOnly="{Boolean}true"/>
                                            <hideinnav
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="hideInNav"
                                                name="./hideInNav"
                                                renderReadOnly="{Boolean}true"
                                                text="Hide in Navigation"
                                                value="true"/>
                                            <showfloatingbrochure
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                fieldDescription="A blank here would mean it would not be preferred over design mode. A value of show/hide would give it preference over design mode settings."
                                                fieldLabel="Show/Hide Floating Brochure"
                                                name="./showFloatingBrochure">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <blank
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Blank (Default)"
                                                        value="blank"/>
                                                    <show
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Show"
                                                        value="show"/>
                                                    <hide
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Hide"
                                                        value="hide"/>
                                                </items>
                                            </showfloatingbrochure>
                                            <whitelistcssselector
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="whitelistSelector"
                                                fieldDescription="Enter a list of class names (without the starting dot) for which the floating brochure must be displayed on the page. Blank here means it would be displayed for the whole page."
                                                fieldLabel="Whitelist for CSS Selectors"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    name="./whitelistSelector"
                                                    required="{Boolean}false"/>
                                            </whitelistcssselector>
                                            <blacklistcssselector
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="blacklistSelector"
                                                fieldDescription="Enter a list of class names (without the starting dot) for which the floating brochure must be hidden from the page. Blank here means it would be hidden for the whole page."
                                                fieldLabel="Blacklist for CSS Selectors"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    name="./blacklistSelector"
                                                    required="{Boolean}false"/>
                                            </blacklistcssselector>
                                            <showfloatingbar
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="charset"
                                                sling:resourceType="granite/ui/components/foundation/form/radiogroup"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="showFloatingBar"
                                                renderReadOnly="{Boolean}true"
                                                text="Show Floating Bar">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <show
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/radio"
                                                        name="./showFloatingBar"
                                                        text="Show"
                                                        value="{Boolean}true"/>
                                                    <hide
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/radio"
                                                        name="./showFloatingBar"
                                                        text="Hide"
                                                        value="{Boolean}false"/>
                                                </items>
                                            </showfloatingbar>
                                            <charset
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="_charset_"
                                                value="utf-8"/>
                                        </items>
                                    </title>
                                    <moretitles
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="More Titles and Description"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagetitle
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="pageTitle"
                                                fieldLabel="Page Title"
                                                name="./pageTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <navigationtitle
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="navTitle"
                                                fieldLabel="Navigation Title"
                                                name="./navTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <subtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="subtitle"
                                                fieldLabel="Subtitle"
                                                name="./subtitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="jcr:description"
                                                fieldLabel="Description"
                                                name="./jcr:description"
                                                renderReadOnly="{Boolean}true"/>
                                            <pagedescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="pagedescription"
                                                fieldLabel="Page description"
                                                name="./pagedescription"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </moretitles>
                                    <onofftime
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="On/Off Time"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ondate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/datepicker"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="onTime"
                                                displayedFormat="MMMM DD, YYYY hh:mm a"
                                                displayTimezoneMessage="{Boolean}true"
                                                fieldLabel="On Time"
                                                name="./onTime"
                                                renderReadOnly="{Boolean}true"
                                                type="datetime"/>
                                            <ondate_type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="./onTime@TypeHint"
                                                value="Date"/>
                                            <offdate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/datepicker"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="offTime"
                                                displayedFormat="MMMM DD, YYYY hh:mm a"
                                                displayTimezoneMessage="{Boolean}true"
                                                fieldLabel="Off Time"
                                                name="./offTime"
                                                renderReadOnly="{Boolean}true"
                                                type="datetime"/>
                                            <offdate_type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="./offTime@TypeHint"
                                                value="Date"/>
                                        </items>
                                    </onofftime>
                                    <vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Vanity URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="sling:vanityPath"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"/>
                                            </vanitypath>
                                            <redirectVanityURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="./sling:redirect"
                                                name="./sling:redirect"
                                                renderReadOnly="{Boolean}true"
                                                text="Redirect Vanity URL"
                                                value="true"/>
                                        </items>
                                    </vanityurl>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <advanced
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Advanced"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section1
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Settings"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <language
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                class="language"
                                                cq-msm-lockable="jcr:language"
                                                emptyText="Select"
                                                fieldLabel="Language"
                                                name="./jcr:language"
                                                renderReadOnly="{Boolean}true"
                                                translateOptions="{Boolean}true">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/common/datasources/languages"
                                                    addNone="{Boolean}true"/>
                                            </language>
                                            <redirect
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                allowBulkEdit="true"
                                                cq-msm-lockable="redirectTarget"
                                                fieldLabel="Redirect"
                                                name="./redirectTarget"
                                                readOnlyURITemplate="/libs/wcm/core/content/sites/properties.html{+value}"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/content"/>
                                            <designs
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                cq-msm-lockable="cq:designPath"
                                                fieldLabel="Design"
                                                name="./cq:designPath"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/etc/designs"/>
                                            <alias
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                cq-msm-lockable="sling:alias"
                                                fieldLabel="Alias"
                                                name="./sling:alias"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </section1>
                                    <templates
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Templates Settings"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <allowedtemplates
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                class="foundation-layout-util-maximized-alt"
                                                fieldDescription="Click 'Add Field' to add a template or a list of templates to be allowed as child pages.                                                 Each value in the list must be an absolute path to a template or use '/.*' to allow all templates below this path."
                                                fieldLabel="Allowed Templates"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    name="./cq:allowedTemplates"
                                                    renderReadOnly="{Boolean}true"/>
                                            </allowedtemplates>
                                        </items>
                                    </templates>
                                    <cug
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Closed User Group"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <enable
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="cq:cugEnabled"
                                                name="./cq:cugEnabled"
                                                renderReadOnly="{Boolean}true"
                                                text="Enabled"
                                                value="true"/>
                                            <deleteenable
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                name="./cq:cugEnabled@Delete"
                                                value="true"/>
                                            <loginpage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                cq-msm-lockable="cq:cugLoginPage"
                                                fieldLabel="Login Page"
                                                name="./cq:cugLoginPage"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/content"/>
                                            <admittedgroups
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="cq:cugPrincipals"
                                                fieldLabel="Admitted Groups"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    name="./cq:cugPrincipals">
                                                    <datasource
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/authorizable/selectdatasource"
                                                        query="\{&quot;selector&quot;: &quot;group&quot;}"/>
                                                </field>
                                            </admittedgroups>
                                            <realm
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                cq-msm-lockable="cq:cugRealm"
                                                fieldDescription="Leave empty to use the page title"
                                                fieldLabel="Realm"
                                                name="./cq:cugRealm"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </cug>
                                    <export
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Export"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <config
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                cq-msm-lockable="cq:exportTemplate"
                                                fieldLabel="Export Configuration"
                                                name="./cq:exportTemplate"
                                                optionLoader="cq.exportTemplate"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/etc/contentsync/templates"/>
                                        </items>
                                    </export>
                                </items>
                            </column>
                        </items>
                    </advanced>
                    <analyticsandseo
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Analytics &amp; Seo"
                        sling:orderBefore="thumbnail"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <notinsitemap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="notInSitemap"
                                        name="./notInSitemap"
                                        renderReadOnly="{Boolean}true"
                                        text="Not in sitemap"
                                        value="true"/>
                                    <notindexed
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="notIndexed"
                                        name="./notIndexed"
                                        renderReadOnly="{Boolean}true"
                                        text="Not indexed"
                                        value="true"/>
                                    <pageCategory1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="pageCategory1"
                                        fieldLabel="Page category 1"
                                        name="./pageCategory1"
                                        renderReadOnly="{Boolean}true"/>
                                    <pageCategory2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="pageCategory2"
                                        fieldLabel="Page category 2"
                                        name="./pageCategory2"
                                        renderReadOnly="{Boolean}true"/>
                                    <pageCategory3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="pageCategory3"
                                        fieldLabel="Page category 3"
                                        name="./pageCategory3"
                                        renderReadOnly="{Boolean}true"/>
                                    <pageevent
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="pageEvent"
                                        fieldDescription="DTM Event"
                                        fieldLabel="DTM Event Value"
                                        name="./pageEvent"
                                        renderReadOnly="{Boolean}true"/>
                                    <appId
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="appId"
                                        fieldLabel="Facebook App ID"
                                        name="./appId"
                                        renderReadOnly="{Boolean}true"/>
                                    <metaTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="metaTitle"
                                        fieldLabel="MetaTitle"
                                        name="./metaTitle"/>
                                    <metaDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="metaDescription"
                                        fieldLabel="MetaDescription"
                                        name="./metaDescription"/>
                                   <canonicalURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="canonicalURL"
                                        fieldLabel="canonicalURL"
                                        name="./canonicalURL"/>
                                </items>
                            </column>
                        </items>
                    </analyticsandseo>
                    <thumbnail
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Thumbnail"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <upload
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/fileupload"
                                        autoStart="{Boolean}false"
                                        cq-msm-lockable="/image"
                                        multiple="{Boolean}false"
                                        name="./image/file.sftmp"
                                        sizeLimit="100000000"
                                        title="Upload Image"
                                        uploadUrl="will_be_replaced"/>
                                </items>
                            </column>
                        </items>
                    </thumbnail>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
