<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="SSC - Suite Variation">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <suitevariation
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Suite variation"
                            cq:showOnCreate="{Boolean}false"
                            sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <suitereference
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="suiteReference"
                                            fieldLabel="Suite reference"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/silversea-com"
                                            name="./suiteReference"/>
                                    <suitefeatures
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/multifield"
                                            cq-msm-lockable="suiteFeatures"
                                            fieldLabel="Suite Features"
                                            renderReadOnly="{Boolean}true">
                                        <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Feature"
                                                name="./suiteFeature"
                                                required="{Boolean}true"/>
                                    </suitefeatures>
                                    <suitecategorycodes
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/multifield"
                                            cq-msm-lockable="suiteCategoryCodes"
                                            fieldLabel="Suite Category Codes"
                                            renderReadOnly="{Boolean}true">
                                        <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                name="./suiteCategoryCode"
                                                fieldLabel="Suite Category Code"
                                                required="{Boolean}true"/>
                                    </suitecategorycodes>
                                    <plan
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="plan"
                                            fieldLabel="Plan"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/dam/silversea-com"
                                            name="./plan"/>
                                    <virtualtour
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="virtualTour"
                                            fieldLabel="Virtual Tour"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/dam/silversea-com"
                                            name="./virtualTour"/>
                                    <locationimage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="virtualTour"
                                            fieldLabel="Location Image"
                                            renderReadOnly="{Boolean}true"
                                            rootPath="/content/dam/silversea-com"
                                            name="./locationImage"/>
                                    <bedroomsinformation
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="bedroomsInformation"
                                            fieldLabel="Bedrooms Information"
                                            renderReadOnly="{Boolean}true"
                                            name="./bedroomsInformation"/>
                                    <suitesubtitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                            name="./suiteSubTitle"
                                            fieldLabel="Suite sub title"
                                            required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </suitevariation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
