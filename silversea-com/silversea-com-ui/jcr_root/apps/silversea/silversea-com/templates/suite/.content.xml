<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Allows to manage and display suites"
          jcr:mixinTypes="[mix:lockable]"
          jcr:primaryType="cq:Template"
          jcr:title="SSC - Suite"
          allowedPaths="[/content/silversea-com/.*]"
          ranking="{Long}0">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="silversea/silversea-com/components/pages/suite">
        <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
            <display
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="silversea/silversea-com/components/displayproperties/suite"/>
        </par>
    </jcr:content>
</jcr:root>
