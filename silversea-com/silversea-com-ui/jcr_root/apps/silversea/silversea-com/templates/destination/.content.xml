<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Silversea.com destination template"
    jcr:mixinTypes="[mix:lockable]"
    jcr:primaryType="cq:Template"
    jcr:title="SSC - Destination"
    allowedPaths="[/content/silversea-com/.*]"
    ranking="{Long}0">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="silversea/silversea-com/components/pages/destination">
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            components="[group:displayproperties]">
            <herobanner
                jcr:primaryType="nt:unstructured"
                sling:resourceType="silversea/silversea-com/components/editorial/herobanner" />
            <display
                jcr:primaryType="nt:unstructured"
                sling:resourceType="silversea/silversea-com/components/displayproperties/destination">
                <inlinegallery
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="silversea/silversea-com/components/editorial/inlinegallery" />
                <findyourcruise
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="silversea/silversea-com/components/editorial/findyourcruise" />
            </display>
        </par>
    </jcr:content>
</jcr:root>