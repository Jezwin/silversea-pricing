<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <excursion
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Excursion"
                        cq:showOnCreate="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false" />
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <codeexcursion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="codeExcursion"
                                        fieldLabel="Excursion code"
                                        name="./codeExcursion"
                                        disabled="{Boolean}true"
                                        renderReadOnly="{Boolean}true" />
                                    <apilongdescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="apiLongDescription"
                                        fieldLabel="API Long Description"
                                        name="./apiLongDescription"
                                        renderReadOnly="{Boolean}true"
                                        disabled="{Boolean}true" />
                                    <longdescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        useFixedInlineToolbar="{Boolean}true"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="longDescription"
                                        fieldLabel="Long Description"
                                        renderReadOnly="{Boolean}true"
                                        name="./longDescription" />
                                    <pois
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="pois"
                                        fieldLabel="POIs"
                                        disabled="{Boolean}true"
                                        renderReadOnly="{Boolean}true"
                                        name="./pois" />
                                    <shorexId
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="shorexId"
                                        fieldLabel="ID"
                                        disabled="{Boolean}true"
                                        name="./shorexId" />
                                    <assetselectionreference
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                            allowBulkEdit="{Boolean}true"
                                            cq-msm-lockable="assetSelectionReference"
                                            fieldLabel="Extra Media Gallery"
                                            rootPath="/content/dam/silversea-com"
                                            name="./assetSelectionReference"/>
                                </items>
                            </column>
                        </items>
                    </excursion>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
